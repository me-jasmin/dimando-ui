{
    "name": "dimando-ui",
    "author": "",
    "description": "UI framework test",
    "version": "0.0.41",
    "license": "MIT",
    "homepage": "https://github.com/me-jasmin/dimando-ui",
    "bugs": "https://github.com/me-jasmin/dimando-ui/issues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/me-jasmin/dimando-ui.git"
    },
    "main": "index.cjs.js",
    "module": "index.esm.js",
    "types": "index.d.ts",
    "sideEffects": false,
    "scripts": {
        "test:jest": "jest ./src --config jest.config.ts --passWithNoTests",
        "test:cypress": "cypress open --component",
        "bundle": "rimraf dist && rollup -c --bundleConfigAsCjs",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "tsc": "tsc --noEmit --project \"./tsconfig.json\" && tsc --noEmit --project \"./cypress/tsconfig.json\"",
        "lint:ts": "eslint \"./**/**/*.{ts,tsx,js}\" --fix",
        "lint:scss": "stylelint \"./**/**/*.scss\" --custom-syntax postcss-scss --fix",
        "format:ts": "prettier --config .prettierrc \"./**/**/*.{ts,tsx,js}\" --end-of-line lf --write",
        "format:scss": "prettier --config .prettierrc \"./**/**/*.scss\" --end-of-line lf --write",
        "precommit": "pnpm tsc && pnpm lint:ts && pnpm lint:scss && pnpm format:ts && pnpm format:scss",
        "clean:nm": "rimraf ./node_modules && rimraf ./pnpm-lock.yaml",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pnpm precommit"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/plugin-syntax-jsx": "^7.23.3",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@cypress/webpack-dev-server": "^3.7.0",
        "@cypress/webpack-preprocessor": "^6.0.0",
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@storybook/addon-a11y": "^7.5.3",
        "@storybook/addon-essentials": "^7.5.3",
        "@storybook/addon-interactions": "^7.5.3",
        "@storybook/addon-links": "^7.5.3",
        "@storybook/addon-onboarding": "^1.0.8",
        "@storybook/addon-storysource": "^7.5.3",
        "@storybook/blocks": "^7.5.3",
        "@storybook/preset-create-react-app": "^7.5.3",
        "@storybook/react": "^7.5.3",
        "@storybook/react-webpack5": "^7.5.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.1.0",
        "@testing-library/user-event": "^14.5.1",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-named-exports-order": "^0.0.2",
        "css-loader": "^6.8.1",
        "cypress": "^13.5.1",
        "eslint-plugin-jest": "^27.6.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.15",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss": "^8.4.31",
        "postcss-scss": "^4.0.9",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.4.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-generate-package-json": "^3.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-typescript2": "^0.36.0",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "storybook": "^7.5.3",
        "style-loader": "^3.3.3",
        "stylelint": "^15.11.0",
        "stylelint-config-recommended-scss": "^13.1.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-order": "^6.0.3",
        "stylelint-scss": "^5.3.1",
        "stylelint-selector-bem-pattern": "^3.0.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "webpack": "^5.89.0"
    }
}